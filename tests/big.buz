/* Test program for the Buzen language */
program TheProgram1 ;
var x , y  : integer ;
var z : boolean ;
procedure TheFirstFunction ( k : integer );
        /* Returns the 'div' of a given integer number */
        begin
		x := k div y
        end ;
function TheSecondFunction ( m : boolean ) : integer ;
        begin
                if ( not m )    /* If condition with else */
                then  x := 0
                else
                begin
                        TheFirstFunction ( 2 ) ;
                        y := y + 3
                end
        end ;
begin
        z := TRUE ;
        y := 0 ;
        while y > 10 do TheSecondFunction(z) /* while without parethesis */
end
.
